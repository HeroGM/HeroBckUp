                <!--/IF_NO_IMAGE-->
                <!--IF_IMAGE-->
                <figure>
                    <img src="<!--IMAGE_RELATIVE_URL-->" alt="<!--CHARACTER_NAME-->">
                    <figcaption>&lt;!--IF_IMAGE--&gt;</figcaption>
                </figure>
                <!--/IF_IMAGE-->
            </section>
            <section>
